{
    "manifest_version": 3,
    "name": "Bullish Private Crypto Automation",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting", "storage"],
    "action": {
        "default_popup": "./popup_window/index.html"
    },
    "background": {
      "service_worker": "./background/background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.sandbox.game/en/*"],
            "js": ["./games/sandbox/main.js", "./misc/utils.js"]
        },
        {
            "matches": ["https://play.pixels.xyz/*"],
            "js": ["content.js", "./misc/utils.js"]
        }
    ]
  }
  